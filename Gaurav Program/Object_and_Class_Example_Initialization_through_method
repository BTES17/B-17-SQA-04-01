package Mypackage;

import java.util.Scanner;

public class Student {
String name;
int rollno, marks;// Instance Variable
static String collegename = "MAIMT"; //Static Variable
static double totalmarks = 100; //Static Variable
void enroll()
	{	
		Scanner in = new Scanner(System.in);
		name = in.next();
		rollno = in.nextInt();
		marks = in.nextInt();	
	}
void percentage()
	{
		double per = ((double)marks/totalmarks)*100;
		System.out.println(+per);
	}
void display()
	{
		System.out.println(name+ "\t " +rollno+ "\t\t" +marks+ "\t" +collegename);
	}
public static void main(String[] args) {
		// TODO Auto-generated method stub
	Student obj1 = new Student(); //Local Variable
	Student obj2 = new Student();
	Student obj3 = new Student();
	Student obj4 = new Student();
	Student obj5 = new Student();
	obj1.enroll();
	obj2.enroll();
	obj3.enroll();
	obj4.enroll();
	obj5.enroll();
	System.out.println("Student Details are");
	System.out.println("=======================================");
	System.out.println("Name \t Rollno \tMarks \tCollegename");
	obj1.display();
	obj2.display();
	obj3.display();
	obj4.display();
	obj5.display();
	System.out.println("Percentage are");	
	obj1.percentage();
	obj2.percentage();
	obj3.percentage();
	obj4.percentage();
	obj5.percentage();

	}
}
***
import java.util.Scanner;
public class Take_Something_Return_Something {
int marks(int a, int b)
{
	int c = a+b;
	return c;
}
public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		Take_Something_Return_Something obj1 = new Take_Something_Return_Something();
		Scanner in = new Scanner(System.in);
		int x = in.nextInt();
		int y = in.nextInt();
		int z = obj1.marks(x,y);
		System.out.println(z);
	}
}
