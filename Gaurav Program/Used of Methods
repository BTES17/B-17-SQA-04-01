//Return something but take Nothing

package Mypackage;

import java.util.Scanner;

public class Student_Method_3 
	{
//int a,b;
int z;
int sum()
	{
	int a,b;
	Scanner in = new Scanner(System.in);
	a = in.nextInt();
	b = in.nextInt();
	z = a+b;
	return z;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student_Method_3 obj1 = new Student_Method_3();
		//Scanner in = new Scanner(System.in);
		//int a,b;
		//a = in.nextInt();
		//b = in.nextInt();
		int z = obj1.sum();
		System.out.println(z);
	}
}
